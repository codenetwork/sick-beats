{
  "name": "sick-beats",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "node-sass": "^4.9.1",
    "npm-run-all": "^4.1.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts-ts": "2.16.0"
  },
  "scripts": {
    "start": "run-p scss:watch react:start",
    "build": "run-s scss:build react:build",
    "test": "run-s scss:build react:test",
    "scss:build": "node-sass src/ -o src/",
    "scss:watch": "node-sass src/ -o src/ --watch",
    "react:start": "react-scripts-ts start",
    "react:build": "react-scripts-ts build",
    "react:test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/jest": "^23.1.4",
    "@types/node": "^10.5.1",
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "typescript": "^2.9.2"
  }
}
